{
  "$schema": "http://json.schemastore.org/template",
  "author": "BepInEx",
  "classifications": [ "BepInEx", "BepInEx 6" , "Plugin", "Il2Cpp" ],
  "identity": "BepInEx6.Plugin.Il2Cpp",
  "name": "BepInEx 6 Il2Cpp Plugin Template",
  "shortName": "bep6plugin_il2cpp",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "preferNameDirectory": true,
  "sourceName": "BepInEx6.PluginTemplate.Il2Cpp",
  "symbols": {
    "TargetFramework": {
      "type": "parameter",
      "description": "The target framework for the project",
      "datatype": "text",
      "defaultValue": "netstandard2.1",
      "replaces": "netstandard2.1"
    },
    "Description": {
      "type": "parameter",
      "description": "Plugin description",
      "datatype": "text",
      "replaces": "My first plugin",
      "defaultValue": "My first plugin"
    },
    "Version": {
      "type": "parameter",
      "description": "Plugin version",
      "datatype": "text",
      "replaces": "{version}",
      "defaultValue": "1.0.0"
    }
  }
}